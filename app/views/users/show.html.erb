<div class="jumbotron">
  <h1><%= @user.username %></h1>
</div>

<div class="row">
  <div class="col-sm-12">
    <div class="card">
      <div class="card-header">
        <h2>Playlists</h2>
      </div>
      <div class="card-body">
        <ul>
          <% @user.playlists.each do |playlist| %>
          <li>
            <%= link_to playlist.name, playlist %>
          </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
