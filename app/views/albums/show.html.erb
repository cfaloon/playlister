<div class="jumbotron">
  <h1><%= @album.name %></h1>
   by <%= link_to @album.artist.name, artist_path(@album.artist) %> (<%= link_to @album.label.name, @album.label %>)</h1>
</div>


<div clas="row">
  <div class="col">
    <div class="card">
      <div class="card-header">
        <h2 class="card-title">Songs</h2>
      </div>
      <div class="card-body">
        <ul>
        <% @album.songs.each do |song| %>
        <li><%= link_to song.name, song %></li>
        <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
